<%= form_with model:@tyrant do |f|%>
<%= f.label :Tyrant_Picture%>
<%= f.file_field :avatar%> <br/>
<%= f.label :Name%>
<%= f.text_field :name%><br/>
<%= f.label :Bio%><br/>
<%= f.text_area :bio%><br/>
<%= f.label :Economics_Policy%><br/>
<%= f.text_area :economics_policy%><br/>
<%= f.label :Social_Policy%><br/>
<%= f.text_area :social_policy%><br/>
<%= f.label :Warmongering_Policy%><br/>
<%= f.text_area :warmongering_policy%><br/>
<%=f.submit%>

<%end%>