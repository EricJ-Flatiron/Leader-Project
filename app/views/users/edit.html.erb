<%= form_with model:@user do |f| %>
<%= f.label :username %>
<%= f.text_field :name %>
<%= f.label :password %>
<%= f.password_field :password%>
<%= f.label :password_confirmation%>
<%= f.password_field :password_confirmation%>
<br/>
<%= f.submit %>
<%end%>
<%= button_to "Back", '/profile',method:"get"%>